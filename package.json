{
  "name": "llm-web-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "frontend": "cd frontend && npm install && npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "backend": "cd backend && python manage.py runserver 0.0.0.0:8000",
    "start": "concurrently \"npm run backend\" \"npm run frontend\""
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
